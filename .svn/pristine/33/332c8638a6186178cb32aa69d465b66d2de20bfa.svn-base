<%@ page language="java" pageEncoding="UTF-8"%>
<%@ include file="/common/taglibs.jspf"%>
<table width="800px">
	<tr>
		<td align="center"><label for="textfield">时间：从</label> <input
			class="Wdate" name="fromdate" id="fromdate" type="text"
			value="${param.fromdate}"
